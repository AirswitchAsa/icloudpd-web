{
  "name": "icloudpd-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "next dev",
    "dev:server": "tsx watch src/server/start.ts",
    "build": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "concurrently \"next start\" \"node dist/server/start.js\"",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "^15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "node-pty": "^1.0.0",
    "@iarna/toml": "^2.2.5",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "tsx": "^4.0.0",
    "concurrently": "^8.0.0"
  }
} 